{
    "dependencies": {
        "@coreui/coreui": "^3.3.0",
        "@fortawesome/fontawesome-free": "^5.15.0",
        "perfect-scrollbar": "^1.5.0"
    },
    "scripts": {
        "copy-coreui-css": "cp node_modules/@coreui/coreui/dist/css/coreui.min.css resources/assets/css/",
        "copy-coreui-css-map": "cp node_modules/@coreui/coreui/dist/css/coreui.min.css.map resources/assets/css/",
        "copy-coreui-js": "cp node_modules/@coreui/coreui/dist/js/coreui.bundle.min.js resources/assets/js/",
        "copy-coreui-js-map": "cp node_modules/@coreui/coreui/dist/js/coreui.bundle.min.js.map resources/assets/js/",
        "copy-coreui-utilities": "cp node_modules/@coreui/coreui/dist/js/coreui-utilities.min.js resources/assets/js/",
        "copy-coreui-utilities-map": "cp node_modules/@coreui/coreui/dist/js/coreui-utilities.min.js.map resources/assets/js/",
        "copy-fontawesome-css": "cp node_modules/@fortawesome/fontawesome-free/css/all.min.css resources/assets/fontawesome/css/fontawesome.css",
        "copy-fontawesome-fonts": "cp -r node_modules/@fortawesome/fontawesome-free/webfonts/ resources/assets/fontawesome/",
        "copy-perfect-scrollbar": "cp node_modules/perfect-scrollbar/dist/perfect-scrollbar.min.js resources/assets/js/",
        "copy-perfect-scrollbar-map": "cp node_modules/perfect-scrollbar/dist/perfect-scrollbar.min.js.map resources/assets/js/",
        "copy-assets": "npm run copy-coreui-css && npm run copy-coreui-css-map && npm run copy-coreui-js && npm run copy-coreui-js-map && npm run copy-coreui-utilities && npm run copy-coreui-utilities-map && npm run copy-fontawesome-css && npm run copy-fontawesome-fonts && npm run copy-perfect-scrollbar && npm run copy-perfect-scrollbar-map"
    }
}
